# ===== API KEYS (Keep these secret!) =====
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google Gemini
GOOGLE_API_KEY=your_google_api_key_here

# Hugging Face
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Cohere
COHERE_API_KEY=your_cohere_api_key_here

# Replicate
REPLICATE_API_TOKEN=your_replicate_api_token_here

# OpenRouter (Free Models Available)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# ===== API CONFIGURATIONS =====
# Primary provider (openai, anthropic, google, openrouter, cohere)
PRIMARY_AI_PROVIDER=openrouter

# Fallback providers (comma-separated, in priority order)
FALLBACK_PROVIDERS=openrouter,google,anthropic,cohere

# Model configurations
OPENAI_MODEL=gpt-4
ANTHROPIC_MODEL=claude-3-sonnet-20240229
GOOGLE_MODEL=gemini-pro
OPENROUTER_MODEL=microsoft/wizardlm-2-8x22b:free
COHERE_MODEL=command

# Token limits
MAX_TOKENS=1000
TEMPERATURE=0.7

# Cost optimization
ENABLE_COST_OPTIMIZATION=true
MAX_MONTHLY_SPEND=100

# API rotation
ENABLE_LOAD_BALANCING=true
ROTATE_APIS=true

# ===== DAILY QUOTA LIMITS =====
# OpenAI Daily Limits
OPENAI_DAILY_REQUESTS=3000
OPENAI_DAILY_TOKENS=100000
OPENAI_DAILY_COST=10.00

# Anthropic Daily Limits
ANTHROPIC_DAILY_REQUESTS=5000
ANTHROPIC_DAILY_TOKENS=200000
ANTHROPIC_DAILY_COST=15.00

# Google Daily Limits
GOOGLE_DAILY_REQUESTS=10000
GOOGLE_DAILY_TOKENS=1000000
GOOGLE_DAILY_COST=5.00

# Cohere Daily Limits
COHERE_DAILY_REQUESTS=1000
COHERE_DAILY_TOKENS=50000
COHERE_DAILY_COST=8.00

# Replicate Daily Limits
REPLICATE_DAILY_REQUESTS=100
REPLICATE_DAILY_TOKENS=10000
REPLICATE_DAILY_COST=5.00

# OpenRouter Daily Limits (Free tier)
OPENROUTER_DAILY_REQUESTS=200
OPENROUTER_DAILY_TOKENS=100000
OPENROUTER_DAILY_COST=0.00

# Quota Safety Settings
QUOTA_SAFETY_MARGIN=0.8
EMERGENCY_THRESHOLD=0.95
HOURLY_DISTRIBUTION=0.1